    "pk": 34,
    "fields": {
        "name": "Arrange desired fruit on top of filling, and chill.",
        "description": "Arrange desired fruit on top of filling, and chill.",
        "order": 5,
        "recipe": 14
    }
},
{
    "model": "recipes.recipestep",
    "pk": 35,
    "fields": {
        "name": "Arrange a layer of ice cream sandwiches in the bottom",
        "description": "of a 9x13-inch dish; top with a layer of whipped topping, hot fudge topping, and caramel topping. Repeat layering with remaining ice cream sandwiches, whipped topping, hot fudge topping, and caramel topping, ending with a top layer of whipped topping. Sprinkle with pecans. Cover dish with aluminum foil and freeze until set, at least 30 minutes.",
        "order": 1,
        "recipe": 15
    }
},
